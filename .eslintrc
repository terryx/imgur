{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true,
    "module": true,
    "arrowFunctions": true,
    "blockBindings": true,
    "generators": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "quotes": [2, "single", "avoid-escape"]
  }
}
